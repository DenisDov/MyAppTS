{
	"name": "my_ts_app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"a": "react-native run-android",
		"ar": "react-native run-android --variant=release",
		"s": "react-native run-ios",
		"5s": "react-native run-ios --simulator='iPhone 5s'",
		"start": "react-native start",
		"test": "jest",
    "lint": "eslint 'App/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint App/**/* --ext .ts,.tsx,.js,.jsx,.json --fix"
	},
	"dependencies": {
		"@react-native-community/masked-view": "^0.1.10",
		"@react-navigation/bottom-tabs": "^5.10.2",
		"@react-navigation/native": "^5.8.2",
		"@react-navigation/stack": "^5.11.1",
		"@shopify/restyle": "^1.3.1",
		"@types/react-redux": "^7.1.9",
		"apisauce": "^2.0.0",
		"immer": "^7.0.14",
		"react": "16.13.1",
		"react-native": "0.63.3",
		"react-native-gesture-handler": "^1.8.0",
		"react-native-reanimated": "^1.13.1",
		"react-native-safe-area-context": "^3.1.8",
		"react-native-screens": "^2.12.0",
		"react-redux": "^7.2.2",
		"redux": "^4.0.5",
		"redux-saga": "^1.1.3",
		"reduxsauce": "^1.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/runtime": "^7.8.4",
		"@react-native-community/eslint-config": "^1.1.0",
		"@types/jest": "^25.2.3",
		"@types/react-native": "^0.63.2",
		"@types/react-test-renderer": "^16.9.2",
		"@typescript-eslint/eslint-plugin": "^2.27.0",
		"@typescript-eslint/parser": "^2.27.0",
		"babel-jest": "^25.1.0",
		"eslint": "^6.5.1",
		"husky": ">=4",
		"jest": "^25.1.0",
		"lint-staged": ">=10",
		"metro-react-native-babel-preset": "^0.59.0",
		"prettier": "^2.1.2",
		"react-test-renderer": "16.13.1",
		"redux-logger": "^3.0.6",
		"typescript": "^3.8.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
    "App/**/*.{js,jsx,ts,tsx,json,md}": "prettier --write",
    "/App/*.{ts,tsx,js,jsx,json}": "yarn lint"
  }
}
